<script type="module">
  import { renderFlashNotifications } from "flash-notifications";

  const settings = {{json settings}};
  const baseLocale = {{json help_center.base_locale}};

  renderFlashNotifications(settings, baseLocale);
</script>
<script>
(function() {
  var formId = '{{#if settings.external_support_form_id}}{{settings.external_support_form_id}}{{else}}23381214703004{{/if}}';
  if (!formId) { return; }
  var path = window.location.pathname;
  var formRegex = /\/hc\/[^/]+\/requests(?:\/new)?$/;
  // Only redirect to the default form when no ticket_form_id is present at all.
  // If a ticket_form_id exists in the query (even if different), do not override it.
  var hasAnyTicketFormId = /[?&]ticket_form_id=\d+/.test(window.location.search);
  if (formRegex.test(path) && !hasAnyTicketFormId) {
    var targetPath = path.replace(/\/requests(?:\/new)?$/, '/requests/new');
    var target = targetPath + '?ticket_form_id=' + formId;
    window.location.replace(target);
  }
})();
</script>
